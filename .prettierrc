    "singleQuote": true,
    "semi": true,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 80,
    "parser": "babel",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "overrides": [
        {
          "files": "*.json",
          "options": {
            "parser": "json",
            "singleQuote": false,
            "semi": false
          }
        }
      ]
}
